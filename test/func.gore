var v1 i32 := 86
var v2 str := "some string"

fn test1(a1 i32) {
    printInt(a1)
    printInt(v1)
    printInt(a1)
}

fn test2(a1 str) {
    printStr(a1)
    printStr(v2)
    printStr(a1)
}

fn test3() {
    printStr("40 + 46 = 86")
    printInt(40 + 46)

    printStr("2 - (3 + 3) = -4")
    printInt(2 - (3 + 3))

    printStr("2 - (3 + 3) + -2 * (-1 + 3) = -8")
    printInt(2 - (3 + 3) + -2 * (-1 + 3))

    printStr("2 + 3 * (1 + 2 * 3) = 23")
    printInt(2 + 3 * (1 + 2 * 3))
}

fn main() {
    test1(420)
    printStr("------------")
    test2("some arg")
    printStr("------------")
    test3()
}
