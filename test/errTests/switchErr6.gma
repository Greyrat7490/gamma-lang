/*
cases should always start in a new line or after a ";"
* more readable
* less likely to happen:
    // similar to condSwitchErr3
     if a1 == {
         86: print("86\n")
             print("86\n") : print("-86\n")    // missing condition before
                              |
                              +--- accidentally no expr before ":" / accidentally put ":" there
                                    -> compiler thinks print is the cond
         _: print("default\n")
     }
*/

fn switch(a1 i32) {
    if a1 < {
        86: print("a1 ")
            print("< 86\n") 420: print("< 420\n")
         _: print(">= 420\n")
    }
}

fn main() {
    switch(69)
}
