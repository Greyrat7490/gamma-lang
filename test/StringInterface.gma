enum E u64 {
    A, B(u32)
}

impl E :: String {
    fn to_str(self) -> str {
        if self : E::B(b) {
            ret fmt("E::B({})", b)
        } else {
            ret "E::A"
        }
    }
}

fn main() {
    u u64 := 64
    println(u.to_str())

    i i64 := -69
    println(i.to_str())

    b := true
    println(b.to_str())

    e1 := E::A
    println(e1.to_str())
    e2 := E::B(420)
    println(e2.to_str())
}
