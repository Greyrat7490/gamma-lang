interface Number {
    add(a u64, b u64) -> u64
    sub(a u64, b u64) -> u64
    inc(self) -> u64
    print(a u64)
}

struct Test {
    a u64
}

struct Test2 {
    a u32
}

impl Test :: Number {
    fn add(a u64, b u64) -> u64 {
        ret a + b
    }

    fn sub(a u64, b u64) -> u64 {
        ret a - b
    }

    fn inc(self) -> u64 {
        ret self.a + 1
    }

    fn print(a u64) {
        println(fmt("Test: {}", a))
    }
}

impl Test2 :: Number {
    fn add(a u64, b u64) -> u64 {
        ret a + b
    }

    fn sub(a u64, b u64) -> u64 {
        ret a - b
    }

    fn inc(self) -> u64 {
        ret self.a + 1 as u64
    }

    fn print(a u64) {
        println(fmt("Test2: {}", Test2::sub(a, 5)))
    }
}

/*
fn calc(lhs Number, rhs Number) -> u64 {
    ret lhs.inc + rhs.inc
}
*/

fn main() {
    t := Test{ 32 }
    t2 := Test2{ 30 }

    Test::print(69)
    Test2::print(69)

    println(fmt("{}", Test::add(30, 39)))
    println(fmt("{}", Test::sub(69, 5)))

    /*
