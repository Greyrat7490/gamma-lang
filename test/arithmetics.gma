v1 i32 := 86
v2 i32 := 86

fn test1() {
    print("v1 = -v1 * 2 + v1 = ")
              v1 = -v1 * 2 + v1
    print(itos(v1))
    print(" (expected -86)\n")

    print("v2 = -v2 * 2 + -v2 = ")
              v2 = -v2 * 2 + -v2
    print(itos(v2))
    print(" (expected -258)\n")
}

fn test2() {
    print("-3 * -3 + 2 = ")
    v1 =      -3 * -3 + 2
    print(itos(v1))
    print(" (expected 11)\n")

    print("-3 * 3 + 2 = ")
    v1 =      -3 * 3 + 2
    print(itos(v1))
    print(" (expected -7)\n")

    print("76 + 10 = ")
    v1 =      76 + 10
    print(itos(v1))
    print(" (expected 86)\n")

    print("76 + 10 - 20 = ")
    v1 =      76 + 10 - 20
    print(itos(v1))
    print(" (expected 66)\n")

    print("76 + 10 - 20 * 10 = ")
    v1 =      76 + 10 - 20 * 10
    print(itos(v1))
    print(" (expected -114)\n")

    print("76 + 10 - 20 * 10 / 2 = ")
    v1 =      76 + 10 - 20 * 10 / 2
    print(itos(v1))
    print(" (expected -14)\n")
}

fn test3() {
    print("10 / 2 - 20 / 2 + 6 / 3 = ")
    v1 =      10 / 2 - 20 / 2 + 6 / 3
    print(itos(v1))
    print(" (expected -3)\n")

    print("2 * 2 - 4 * 2 = ")
    v1 =      2 * 2 - 4 * 2
    print(itos(v1))
    print(" (expected -4)\n")

    print("2 * 2 - 4 * 4 + 3 * 3 = ")
    v1 =      2 * 2 - 4 * 4 + 3 * 3
    print(itos(v1))
    print(" (expected -3)\n")
}

fn test4() {
    print("2 - (3 + 3) = ")
    v1 =      2 - (3 + 3)
    print(itos(v1))
    print(" (expected -4)\n")

    print("2 - (3 + 3) + -2 * (-1 + 3) = ")
    v1 =      2 - (3 + 3) + -2 * (-1 + 3)
    print(itos(v1))
    print(" (expected -8)\n")

    print("2 + 3 * (1 + 2 * 3) = ")
    v1 =      2 + 3 * (1 + 2 * 3)
    print(itos(v1))
    print(" (expected 23)\n")
}

fn modTest() {
    print("420 % 2 = ")
    v1 =      420 % 2
    print(itos(v1)) print("\n")

    print("69 % 2 = ")
    v1 =      69 % 2
    print(itos(v1)) print("\n")

    print("86 % 2 = ")
    v1 =      86 % 2
    print(itos(v1)) print("\n")
}

fn main() {
    test1()
    print("\n")
    test2()
    print("\n")
    test3()
    print("\n")
    test4()
    print("\n")
    modTest()
}
