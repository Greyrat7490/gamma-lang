v1 i32 := 420

fn simple() {
    l1 bool := v1 == 86
    printStr("420 == 86: ")
    printBool(l1)
    printStr("\n")

    l1 = v1 != 86
    printStr("420 != 86: ")
    printBool(l1)
    printStr("\n")

    l1 = v1 < 86
    printStr("420 < 86: ")
    printBool(l1)
    printStr("\n")

    l1 = v1 > 86
    printStr("420 > 86: ")
    printBool(l1)
    printStr("\n")

    l1 = v1 <= 86
    printStr("420 <= 86: ")
    printBool(l1)
    printStr("\n")

    l1 = v1 >= 86
    printStr("420 >= 86: ")
    printBool(l1)
    printStr("\n")

    l1 = v1 >= 420
    printStr("420 >= 420: ")
    printBool(l1)
    printStr("\n")

    l1 = v1 >= v1
    printStr("v1 >= v1: ")
    printBool(l1)
    printStr("\n")
}

fn arith() {
    printStr("410 + 10 == 420: ")
    printBool(410 + 10 == 420)
    printStr("\n")

    printStr("410 + 10 != 420: ")
    printBool(410 + 10 != 420)
    printStr("\n")

    printStr("40 * 10 + 20 > 69: ")
    printBool(40 * 10 + 20 > 69)
    printStr("\n")

    printStr("40 * 10 + 20 < 421: ")
    printBool(40 * 10 + 20 < 421)
    printStr("\n")
}

fn main() {
    printStr("simple\n")
    simple()
    printStr("\narith\n")
    arith()
}
