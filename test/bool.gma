v1 i32 := 420

fn simple() {
    l1 bool := v1 == 86
    print("420 == 86: ")
    print(btos(l1))
    print("\n")

    l1 = v1 != 86
    print("420 != 86: ")
    print(btos(l1))
    print("\n")

    l1 = v1 < 86
    print("420 < 86: ")
    print(btos(l1))
    print("\n")

    l1 = v1 > 86
    print("420 > 86: ")
    print(btos(l1))
    print("\n")

    l1 = v1 <= 86
    print("420 <= 86: ")
    print(btos(l1))
    print("\n")

    l1 = v1 >= 86
    print("420 >= 86: ")
    print(btos(l1))
    print("\n")

    l1 = v1 >= 420
    print("420 >= 420: ")
    print(btos(l1))
    print("\n")

    l1 = v1 >= v1
    print("v1 >= v1: ")
    print(btos(l1))
    print("\n")
}

fn arith() {
    print("410 + 10 == 420: ")
    print(btos(410 + 10 == 420))
    print("\n")

    print("410 + 10 != 420: ")
    print(btos(410 + 10 != 420))
    print("\n")

    print("40 * 10 + 20 > 69: ")
    print(btos(40 * 10 + 20 > 69))
    print("\n")

    print("40 * 10 + 20 < 421: ")
    print(btos(40 * 10 + 20 < 421))
    print("\n")
}

fn main() {
    print("simple\n")
    simple()
    print("\narith\n")
    arith()
}
