var v1 i32 := 40 + 46
var v2 i32 := -420

fn test1() {
    printStr("40 + 46")
    printInt(v1)

    printStr("40 + 46 - 10")
    v1 = 40 + 46 - 10
    printInt(v1)

    printStr("40 + 46 - 10 * 2")
    v1 = 40 + 46 - 10 * 2
    printInt(v1)

    printStr("40 + 46 - 10 * 2 / 2")
    v1 = 40 + 46 - 10 * 2 / 2
    printInt(v1)
}

fn main() {
    printInt(v2)

    test1()
}
