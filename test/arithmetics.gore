fn main() {
   // printInt(40 + 46) // not yet supported (TODO)

    var l1 i32

    printStr("76 + 10")                      // 86
    l1 = 76 + 10
    printInt(l1)

    printStr("76 + 10 - 20")                 // 66
    l1 = 76 + 10 - 20
    printInt(l1)

    printStr("76 + 10 - 20 * 10")            // -114
    l1 = 76 + 10 - 20 * 10
    printInt(l1)

    printStr("76 + 10 - 20 * 10 / 2")        // -14
    l1 = 76 + 10 - 20 * 10 / 2
    printInt(l1)

    printStr("10 / 2 - 20 / 2 + 6 / 3")      // -3
    l1 = 10 / 2 - 20 / 2 + 6 / 3
    printInt(l1)
}
