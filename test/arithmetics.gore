var v1 i32 := 86
var v2 i32 := 86

fn test1() {
    printStr("v1 = -v1 * 2 + v1 = ")
              v1 = -v1 * 2 + v1
    printInt(v1)
    printStr(" (expected -86)\n")

    printStr("v2 = -v2 * 2 + -v2 = ")
              v2 = -v2 * 2 + -v2
    printInt(v2)
    printStr(" (expected -258)\n")
}

fn test2() {
    printStr("-3 * -3 + 2 = ")
    v1 =      -3 * -3 + 2
    printInt(v1)
    printStr(" (expected 11)\n")

    printStr("-3 * 3 + 2 = ")
    v1 =      -3 * 3 + 2
    printInt(v1)
    printStr(" (expected -7)\n")

    printStr("76 + 10 = ")
    v1 =      76 + 10
    printInt(v1)
    printStr(" (expected 86)\n")

    printStr("76 + 10 - 20 = ")
    v1 =      76 + 10 - 20
    printInt(v1)
    printStr(" (expected 66)\n")

    printStr("76 + 10 - 20 * 10 = ")
    v1 =      76 + 10 - 20 * 10
    printInt(v1)
    printStr(" (expected -114)\n")

    printStr("76 + 10 - 20 * 10 / 2 = ")
    v1 =      76 + 10 - 20 * 10 / 2
    printInt(v1)
    printStr(" (expected -14)\n")
}

fn test3() {
    printStr("10 / 2 - 20 / 2 + 6 / 3 = ")
    v1 =      10 / 2 - 20 / 2 + 6 / 3
    printInt(v1)
    printStr(" (expected -3)\n")

    printStr("2 * 2 - 4 * 2 = ")
    v1 =      2 * 2 - 4 * 2
    printInt(v1)
    printStr(" (expected -4)\n")

    printStr("2 * 2 - 4 * 4 + 3 * 3 = ")
    v1 =      2 * 2 - 4 * 4 + 3 * 3
    printInt(v1)
    printStr(" (expected -3)\n")
}

fn test4() {
    printStr("2 - (3 + 3) = ")
    v1 =      2 - (3 + 3)
    printInt(v1)
    printStr(" (expected -4)\n")

    printStr("2 - (3 + 3) + -2 * (-1 + 3) = ")
    v1 =      2 - (3 + 3) + -2 * (-1 + 3)
    printInt(v1)
    printStr(" (expected -8)\n")

    printStr("2 + 3 * (1 + 2 * 3) = ")
    v1 =      2 + 3 * (1 + 2 * 3)
    printInt(v1)
    printStr(" (expected 23)\n")
}

fn modTest() {
    printStr("420 % 2 = ")
    v1 =      420 % 2
    printInt(v1) printStr("\n")

    printStr("69 % 2 = ")
    v1 =      69 % 2
    printInt(v1) printStr("\n")

    printStr("86 % 2 = ")
    v1 =      86 % 2
    printInt(v1) printStr("\n")
}

fn main() {
    test1()
    printStr("\n")
    test2()
    printStr("\n")
    test3()
    printStr("\n")
    test4()
    printStr("\n")
    modTest()
}
