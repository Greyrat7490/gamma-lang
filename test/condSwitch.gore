fn simple(a1 i32) {
    if a1 == {
        420: printStr("a1 == 420\n")
        86:  printStr("a1 == 86\n")
        -86: printStr("a1 == -86\n")
        _:   printStr("a1 something else\n")
    }
}

fn onlyDefault(a1 i32) {
    if a1 == {
        _:   printStr("default\n")
    }
}

fn arith(a1 i32) {
    if a1 * 10 + 10 == {
        420:  printStr("a1 == 420\n")
        860:  printStr("a1 == 860\n")
        -860: printStr("a1 == -860\n")
        _:    printStr("a1 something else\n")
    }
}

fn lss(a1 i32) {
    if a1 < {
        -86: printStr("a1 < -86\n")
        420: printStr("a1 < 420\n")
        86:  printStr("a1 < 86\n")          // gets never called because 420 > 86 and goes from top to bottom
        _:   printStr("a1 something else\n")
    }
}

fn stmts(a1 i32) {
    if a1 < {
        -86:
            printInt(a1)
            printStr(" < -86\n")
        420:
            printInt(a1)
            printStr(" < 420\n")
        86:
            printInt(a1)
            printStr(" < 86\n")
        _:
            printInt(a1)
            printStr(" >= 420\n")
    }
}

fn main() {
    printStr("---- simple ----\n")
    simple(86)
    simple(-86)
    simple(420)
    simple(69)

    printStr("---- only default ----\n")
    onlyDefault(86)
    onlyDefault(-86)
    onlyDefault(420)
    onlyDefault(69)

    printStr("---- arith in base cond ----\n")
    arith(85)
    arith(-87)
    arith(41)
    arith(69)

    printStr("---- lesser ----\n")
    lss(86)
    lss(-87)
    lss(420)
    lss(69)

    printStr("---- multiple stmts ----\n")
    stmts(86)
    stmts(-87)
    stmts(420)
    stmts(69)
}
